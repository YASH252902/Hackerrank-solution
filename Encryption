#include <iostream>
#include <cmath>
#include <string>

using namespace std;

int main() {
    string s;
    getline(cin, s);

    // Remove spaces from the string if any
    string str;
    for (char c : s) {
        if (c != ' ') {
            str += c;
        }
    }

    int L = str.length();
    int rows = floor(sqrt(L));
    int cols = ceil(sqrt(L));

    if (rows * cols < L) {
        rows += 1;
    }

    // Print encrypted message
    for (int c = 0; c < cols; ++c) {
        for (int r = 0; r < rows; ++r) {
            int idx = r * cols + c;
            if (idx < L) {
                cout << str[idx];
            }
        }
        cout << " ";
    }

    cout << "\n";
    return 0;
}
