#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int q; // number of test cases
    cin >> q;
    while (q--) {
        int n, k;
        cin >> n >> k;
        vector<int> A(n), B(n);

        for (int i = 0; i < n; i++) cin >> A[i];
        for (int i = 0; i < n; i++) cin >> B[i];

        sort(A.begin(), A.end());               // ascending
        sort(B.begin(), B.end(), greater<int>()); // descending

        bool possible = true;
        for (int i = 0; i < n; i++) {
            if (A[i] + B[i] < k) {
                possible = false;
                break;
            }
        }

        cout << (possible ? "YES" : "NO") << "\n";
    }

    return 0;
}
